# Production Environment Variables
# Copy this to .env.production and fill in the values

# Database
PRODUCTION_DB_PASSWORD=your_secure_production_db_password_here

# JWT Secrets (generate with: openssl rand -hex 32)
PRODUCTION_JWT_ACCESS_SECRET=your_production_jwt_access_secret_here
PRODUCTION_JWT_REFRESH_SECRET=your_production_jwt_refresh_secret_here

# Grafana
PRODUCTION_GRAFANA_ADMIN_USER=admin
PRODUCTION_GRAFANA_ADMIN_PASSWORD=your_secure_grafana_password_here

# API URLs
VITE_API_URL=https://spherosegapp.utia.cas.cz/api
VITE_ML_SERVICE_URL=https://spherosegapp.utia.cas.cz/ml
VITE_WS_URL=https://spherosegapp.utia.cas.cz

# Email Service (optional)
EMAIL_HOST=smtp.gmail.com
EMAIL_PORT=587
EMAIL_USER=your_email@gmail.com
EMAIL_PASSWORD=your_app_specific_password

# S3 Storage (optional)
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_REGION=eu-central-1
S3_BUCKET_NAME=spheroseg-production

# Monitoring
SENTRY_DSN=
NEW_RELIC_LICENSE_KEY=