{
  "*.{ts,tsx}": [
    "eslint --fix --cache --max-warnings=10 --format=stylish",
    "prettier --write"
  ],
  "*.{js,jsx}": [
    "eslint --fix --cache --max-warnings=10 --format=stylish",
    "prettier --write"
  ],
  "*.{css,scss}": ["prettier --write"],
  "*.{json,md,html,yml,yaml}": ["prettier --write"]
}
